{
  "name": "peltonsolutions/laravel-enums",
  "description": "A custom package for Laravel for handling enum values on Models.",
  "keywords": [
    "peltonsolutions",
    "laravel",
    "laravel-enums"
  ],
  "homepage": "https://github.com/peltonsolutions/laravel-enums",
  "license": "MIT",
  "authors": [
    {
      "name": "Nathan Pelton",
      "email": "npelton@gmail.com",
      "homepage": "https://www.nathanpelton.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.2",
    "laravel/framework": "^10.10"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5|^9.5.10|^10.0"
  },
  "autoload": {
    "psr-4": {
      "PeltonSolutions\\LaravelEnums\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PeltonSolutions\\LaravelEnums\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "watch": "vendor/bin/phpunit-watcher watch"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "PeltonSolutions\\LaravelEnums\\LaravelEnumsServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
